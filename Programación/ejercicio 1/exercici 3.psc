Proceso sin_titulo
	Definir numero1, numero2 como entero;
	Leer numero1;
	Leer numero2;
	Escribir "Suma ", numero1 + numero2;
	Escribir "Resta ", numero1 - numero2;
	Escribir "Mutiplicación ", numero1 * numero2;
	Escribir "División ", numero1 / numero2;
	Escribir "Modulo ", numero1 % numero2;
FinProceso
